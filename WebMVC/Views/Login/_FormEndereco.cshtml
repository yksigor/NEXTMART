@model Domain.Models.Endereco

<div class="form-row">
    <div class="form-group col-md-4">
        <label asp-for="CEP" class="control-label"></label>
        <input asp-for="CEP" class="form-control" onblur="buscarCEP()" />
    </div>
    <div class="form-group col-md-6">
        <label asp-for="Municipio" class="control-label"></label>
        <input asp-for="Municipio" class="form-control" />
    </div>
    <div class="form-group col-md-2">
        <label asp-for="UF" class="control-label"></label>
        <select asp-for="UF" class="form-control" asp-items="new SelectList(@Model.GetUFs())">
            <option selected></option>
        </select>
    </div>
</div>
<div class="form-row">
    <div class="form-group col-md-9">
        <label asp-for="Logradouro" class="control-label"></label>
        <input asp-for="Logradouro" class="form-control" />
    </div>
    <div class="form-group col-md-3">
        <label asp-for="Numero" class="control-label"></label>
        <input asp-for="Numero" class="form-control" />
    </div>
</div>
<div class="form-row">
    <div class="form-group col-md-6">
        <label asp-for="Complemento" class="control-label"></label>
        <input asp-for="Complemento" class="form-control" />
    </div>
    <div class="form-group col-md-6">
        <label asp-for="Bairro" class="control-label"></label>
        <input asp-for="Bairro" class="form-control" />
    </div>
</div>