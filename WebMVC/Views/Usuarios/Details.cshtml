@model Domain.Models.Usuario

@{
    Layout = null;
}

<h1>Detalhes</h1>

<div>
    <h4>Usuario</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Usuário
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Username)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            Senha
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Password)
        </dd>
        <dt class="col-sm-2">
            Tipo do Usuário
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TipoUsuario)
        </dd>
    </dl>
</div>
<div>
    <a data-dismiss="modal" class="btn btn-danger">Voltar</a>
</div>